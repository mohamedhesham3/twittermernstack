{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/API/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.html"
    }
  ]
}
